{"ast":null,"code":"import { ref } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = todo => {\n      todos.value.push(todo);\n    };\n\n    const toggleTodo = index => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,SAASA,GAAT,QAAmB,KAAnB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF,kBADU;AAEVC;AAFU,GADC;;AAKbE,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIL,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMM,OAAM,GAAKC,IAAD,IAAU;AACxBF,WAAK,CAACG,KAAN,CAAYC,IAAZ,CAAiBF,IAAjB;AACD,KAFD;;AAIA,UAAMG,UAAS,GAAKC,KAAD,IAAW;AAC5BN,WAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBC,SAAnB,GAA+B,CAACP,KAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBC,SAAnD;AACD,KAFD;;AAIA,UAAMC,UAAS,GAAKF,KAAD,IAAW;AAC5BN,WAAK,CAACG,KAAN,CAAYM,MAAZ,CAAmBH,KAAnB,EAA0B,CAA1B;AACD,KAFD;;AAIA,UAAMI,UAAS,GAAIf,GAAG,CAAC,EAAD,CAAtB;AAEA,WAAO;AACLK,WADK;AAELC,aAFK;AAGLO,gBAHK;AAILH,gBAJK;AAKLK;AALK,KAAP;AAOF;;AA7Ba,CAAf","names":["ref","TodoSimpleForm","TodoList","components","setup","todos","addTodo","todo","value","push","toggleTodo","index","completed","deleteTodo","splice","searchText"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n\n    <h2>To-Do List</h2>\n    <input\n        class=\"form-control\"\n        type=\"text\" \n        v-model=\"searchText\"\n        placeholder=\"search\"\n    >\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    \n    <div v-if=\"!todos.length\">\n      추가된 Todo가 없습니다\n    </div>\n    <TodoList :todos=\"todos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport { ref} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList,\n  },\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = (todo) => {\n      todos.value.push(todo);\n    };\n\n    const toggleTodo = (index) => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}