{"ast":null,"code":"import { ref, computed, onUnmounted } from 'vue';\nimport { useRoute, useRouter } from 'vue-router';\nimport axios from 'axios';\nimport _ from 'lodash';\nimport Toast from '@/components/Toast.vue';\nimport { useToast } from '@/hooks/toast';\nexport default {\n  components: {\n    Toast\n  },\n\n  setup() {\n    const route = useRoute();\n    const router = useRouter();\n    const todo = ref(null);\n    const originTodo = ref(null);\n    const loading = ref(true);\n    const todoId = route.params.id; // const showToast = ref(false);\n    // const toastMessage = ref('');\n    // const toastAlertType = ref('');\n    // const timeout = ref(null);\n    // const triggerToast = (message, type='success') => {\n    //   toastMessage.value = message;\n    //   toastAlertType.value = type;\n    //   showToast.value = true;\n    //   timeout.value = setTimeout(() => {\n    //     console.log('hello');\n    //     toastMessage.value = '';\n    //     toastAlertType.value = '';\n    //     showToast.value = false;\n    //   },5000)\n    // }\n\n    onUnmounted(() => {\n      console.log('unmout');\n    });\n    const {\n      toastMessage,\n      toastAlertType,\n      showToast,\n      triggerToast\n    } = useToast();\n\n    const getTodo = async () => {\n      try {\n        const res = await axios.get('http://localhost:3000/todos/' + todoId);\n        todo.value = { ...res.data\n        };\n        originTodo.value = { ...res.data\n        };\n        loading.value = false;\n      } catch (err) {\n        triggerToast('error', 'danger');\n      }\n    };\n\n    const todoUpdated = computed(() => {\n      return !_.isEqual(todo.value, originTodo.value);\n    });\n\n    const toggleTodoStatus = () => {\n      todo.value.completed = !todo.value.completed;\n    };\n\n    const moveToTodoListPage = () => {\n      router.push({\n        name: 'Todos'\n      });\n    };\n\n    const onSave = async () => {\n      try {\n        const res = await axios.put('http://localhost:3000/todos/' + todoId, {\n          subject: todo.value.subject,\n          completed: todo.value.completed\n        });\n        originTodo.value = { ...res.data\n        };\n        triggerToast('success save');\n      } catch (err) {\n        triggerToast('error', 'danger');\n      }\n    };\n\n    getTodo();\n    return {\n      todo,\n      loading,\n      toggleTodoStatus,\n      moveToTodoListPage,\n      onSave,\n      todoUpdated,\n      showToast,\n      toastMessage,\n      toastAlertType\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAgCA,SAAQA,GAAR,EAAaC,QAAb,EAAuBC,WAAvB,QAAyC,KAAzC;AACA,SAAQC,QAAR,EAAkBC,SAAlB,QAAkC,YAAlC;AACA,OAAOC,KAAP,MAAkB,OAAlB;AACA,OAAOC,CAAP,MAAc,QAAd;AACA,OAAOC,KAAP,MAAkB,wBAAlB;AACA,SAAQC,QAAR,QAAuB,eAAvB;AAEA,eAAe;AACXC,YAAU,EAAC;AACTF;AADS,GADA;;AAIXG,OAAK,GAAE;AACL,UAAMC,KAAI,GAAIR,QAAQ,EAAtB;AACA,UAAMS,MAAK,GAAIR,SAAS,EAAxB;AACA,UAAMS,IAAG,GAAIb,GAAG,CAAC,IAAD,CAAhB;AACA,UAAMc,UAAS,GAAId,GAAG,CAAC,IAAD,CAAtB;AACA,UAAMe,OAAM,GAAIf,GAAG,CAAC,IAAD,CAAnB;AACA,UAAMgB,MAAK,GAAIL,KAAK,CAACM,MAAN,CAAaC,EAA5B,CANK,CAQL;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEAhB,eAAW,CAAC,MAAI;AACdiB,aAAO,CAACC,GAAR,CAAY,QAAZ;AACD,KAFU,CAAX;AAIA,UAAK;AACHC,kBADG;AAEHC,oBAFG;AAGHC,eAHG;AAIHC;AAJG,QAKHhB,QAAQ,EALV;;AAOA,UAAMiB,OAAM,GAAI,YAAY;AAC1B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMrB,KAAK,CAACsB,GAAN,CAAU,iCAA+BX,MAAzC,CAAlB;AAEAH,YAAI,CAACe,KAAL,GAAa,EAAC,GAAGF,GAAG,CAACG;AAAR,SAAb;AACAf,kBAAU,CAACc,KAAX,GAAmB,EAAC,GAAGF,GAAG,CAACG;AAAR,SAAnB;AACAd,eAAO,CAACa,KAAR,GAAgB,KAAhB;AACD,OAND,CAMC,OAAME,GAAN,EAAU;AACTN,oBAAY,CAAC,OAAD,EAAS,QAAT,CAAZ;AACF;AAEF,KAXA;;AAaA,UAAMO,WAAU,GAAI9B,QAAQ,CAAC,MAAM;AACjC,aAAO,CAACK,CAAC,CAAC0B,OAAF,CAAUnB,IAAI,CAACe,KAAf,EAAqBd,UAAU,CAACc,KAAhC,CAAR;AACD,KAF2B,CAA5B;;AAIA,UAAMK,gBAAe,GAAI,MAAM;AAC7BpB,UAAI,CAACe,KAAL,CAAWM,SAAX,GAAuB,CAACrB,IAAI,CAACe,KAAL,CAAWM,SAAnC;AACF,KAFA;;AAIA,UAAMC,kBAAiB,GAAI,MAAM;AAC/BvB,YAAM,CAACwB,IAAP,CAAY;AACVC,YAAI,EAAC;AADK,OAAZ;AAGF,KAJA;;