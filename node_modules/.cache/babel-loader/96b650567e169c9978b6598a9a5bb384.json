{"ast":null,"code":"import { ref } from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\nexport default {\n  components: {\n    SimpleForm\n  },\n\n  setup() {\n    // const completed = ref(false);\n    const todos = ref([]);\n\n    const onSubmit = () => {\n      if (todo.value) {\n        hasError.value = false;\n        todos.value.push({\n          id: Date.now(),\n          contents: todo.value,\n          completed: false\n        });\n        todo.value = \"\";\n      } else {\n        hasError.value = true;\n      }\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    return {\n      onSubmit,\n      todos,\n      deleteTodo\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA4BA,SAAQA,GAAR,QAAkB,KAAlB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,OAAK,GAAE;AACL;AAEA,UAAMC,KAAI,GAAIJ,GAAG,CAAC,EAAD,CAAjB;;AACA,UAAMK,QAAO,GAAI,MAAM;AACrB,UAAGC,IAAI,CAACC,KAAR,EAAc;AACZC,gBAAQ,CAACD,KAAT,GAAe,KAAf;AACAH,aAAK,CAACG,KAAN,CAAYE,IAAZ,CAAiB;AAEfC,YAAE,EAAEC,IAAI,CAACC,GAAL,EAFW;AAGfC,kBAAQ,EAAEP,IAAI,CAACC,KAHA;AAIfO,mBAAS,EAAE;AAJI,SAAjB;AAMAR,YAAI,CAACC,KAAL,GAAW,EAAX;AACD,OATD,MASK;AACHC,gBAAQ,CAACD,KAAT,GAAe,IAAf;AACF;AACF,KAbA;;AAeA,UAAMQ,UAAS,GAAKC,KAAD,IAAW;AAC5BZ,WAAK,CAACG,KAAN,CAAYU,MAAZ,CAAmBD,KAAnB,EAAyB,CAAzB;AACF,KAFA;;AAIA,WAAM;AACJX,cADI;AAGJD,WAHI;AAIJW;AAJI,KAAN;AAOF;;AAlCa,CAAf","names":["ref","SimpleForm","components","setup","todos","onSubmit","todo","value","hasError","push","id","Date","now","contents","completed","deleteTodo","index","splice"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>To-Do List</h2>\n    <SimpleForm />\n    \n    <div v-if=\"hasError\">\n      Error\n    </div>\n    <div v-if=\"!todos.length\">\n      nothing\n    </div>\n    <div class=\"card mt-2\" v-for=\"(list,index) in todos\" :key=\"index\">\n      <div class=\"card-body p-2 d-flex align-items-center\">\n        <div class=\"form-check flex-grow-1 \">\n          <input class=\"form-check-input\" type=\"checkbox\" v-model=\"list.completed\">\n          <label class=\"form-check-label\" :class=\"{todo:list.completed}\"> {{list.contents}}</label>\n        </div>\n        <div>\n          <button class=\"btn btn-danger btn-sm\" @click=\"deleteTodo(index)\">X</button>\n        </div>\n      </div>\n      \n    </div>\n    \n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\n\nexport default {\n  components: {\n    SimpleForm\n  },\n  setup(){\n    // const completed = ref(false);\n    \n    const todos = ref([]);\n    const onSubmit = () => {\n      if(todo.value){\n        hasError.value=false\n        todos.value.push({\n          \n          id: Date.now(),\n          contents: todo.value,\n          completed: false,\n        })\n        todo.value=\"\";\n      }else{\n        hasError.value=true\n      }\n    }\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index,1);\n    }\n\n    return{\n      onSubmit,\n     \n      todos,\n      deleteTodo\n      \n    }\n  }\n}\n</script>\n\n<style>\n  .todo{\n    text-decoration: line-through;\n    color: gray;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}