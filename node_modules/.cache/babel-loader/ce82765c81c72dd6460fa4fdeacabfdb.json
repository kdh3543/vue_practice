{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport TodoPage from './components/TodoPage.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList,\n    TodoPage\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos?_page=1&_limit=5', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        error.value = err;\n      } // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n\n    };\n\n    const getTodos = async (page = currentPage.value) => {\n      currentPage.value = page;\n\n      try {\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`);\n        numberOfTodos.value = res.headers['x-total-count'];\n        todos.value = res.data;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    getTodos();\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuBA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVJ,kBADU;AAEVC,YAFU;AAGVC;AAHU,GADC;;AAMbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMS,KAAI,GAAIT,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMU,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,IAAN,CAAW,8CAAX,EAA0D;AAC1EC,iBAAO,EAAEH,IAAI,CAACG,OAD4D;AAE1EC,mBAAS,EAAEJ,IAAI,CAACI;AAF0D,SAA1D,CAAlB;AAIAP,aAAK,CAACQ,KAAN,CAAYC,IAAZ,CAAiBL,GAAG,CAACM,IAArB;AACD,OAND,CAMC,OAAMC,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF,OAT8B,CAU9B;AACA;AACA;AACA;AACA;;AACD,KAfD;;AAiBA,UAAMC,QAAO,GAAI,OAAMC,IAAG,GAAIC,WAAW,CAACN,KAAzB,KAAmC;AAClDM,iBAAW,CAACN,KAAZ,GAAoBK,IAApB;;AACA,UAAG;AACD,cAAMT,GAAE,GAAI,MAAMP,KAAK,CAACkB,GAAN,CAAW,qCAAoCF,IAAK,WAAUG,KAAM,EAApE,CAAlB;AACAC,qBAAa,CAACT,KAAd,GAAsBJ,GAAG,CAACc,OAAJ,CAAY,eAAZ,CAAtB;AACAlB,aAAK,CAACQ,KAAN,GAAcJ,GAAG,CAACM,IAAlB;AACD,OAJD,CAIC,OAAMC,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF;AACF,KATA;;AAUAC,YAAQ;;AAER,UAAMO,UAAS,GAAI,MAAOC,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIrB,KAAK,CAACQ,KAAN,CAAYY,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAMxB,KAAK,CAACyB,KAAN,CAAY,iCAA+BD,EAA3C,EAA8C;AAClDd,mBAAS,EAAE,CAACP,KAAK,CAACQ,KAAN,CAAYY,KAAZ,EAAmBb;AADmB,SAA9C,CAAN;AAGAP,aAAK,CAACQ,KAAN,CAAYY,KAAZ,EAAmBb,SAAnB,GAA+B,CAACP,KAAK,CAACQ,KAAN,CAAYY,KAAZ,EAAmBb,SAAnD;AACD,OALD,CAKC,OAAMI,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF;AAED,KAXD;;AAaA,UAAMY,UAAS,GAAI,MAAOH,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIrB,KAAK,CAACQ,KAAN,CAAYY,KAAZ,E