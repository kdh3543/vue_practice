{"ast":null,"code":"import { ref } from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nexport default {\n  components: {\n    SimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const addTodo = todo => {\n      todos.value.push(todo);\n    };\n\n    const toggleTodo = index => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    return {\n      todos,\n      deleteTodo,\n      addTodo,\n      toggleTodo\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA,SAAQA,GAAR,QAAkB,KAAlB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF,cADU;AAEVC;AAFU,GADC;;AAKbE,OAAK,GAAE;AACL,UAAMC,KAAI,GAAIL,GAAG,CAAC,EAAD,CAAjB;;AACA,UAAMM,UAAS,GAAKC,KAAD,IAAW;AAC5BF,WAAK,CAACG,KAAN,CAAYC,MAAZ,CAAmBF,KAAnB,EAAyB,CAAzB;AACF,KAFA;;AAGA,UAAMG,OAAM,GAAKC,IAAD,IAAU;AACxBN,WAAK,CAACG,KAAN,CAAYI,IAAZ,CAAiBD,IAAjB;AACF,KAFA;;AAGA,UAAME,UAAS,GAAKN,KAAD,IAAW;AAC5BF,WAAK,CAACG,KAAN,CAAYD,KAAZ,EAAmBO,SAAnB,GAA+B,CAACT,KAAK,CAACG,KAAN,CAAYD,KAAZ,EAAmBO,SAAnD;AACF,KAFA;;AAGA,WAAM;AACJT,WADI;AAEJC,gBAFI;AAGJI,aAHI;AAIJG;AAJI,KAAN;AAMF;;AAtBa,CAAf","names":["ref","SimpleForm","TodoList","components","setup","todos","deleteTodo","index","value","splice","addTodo","todo","push","toggleTodo","completed"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>To-Do List</h2>\n    <SimpleForm @add=\"addTodo\"/>\n    \n    \n    <div v-if=\"!todos.length\">\n      nothing\n    </div>\n    <TodoList :todos=\"todos\" @toggle-todo=\"toggleTodo\"/>\n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\n\nexport default {\n  components: {\n    SimpleForm,\n    TodoList,\n  },\n  setup(){\n    const todos = ref([]);\n    const deleteTodo = (index) => {\n      todos.value.splice(index,1);\n    }\n    const addTodo = (todo) => {\n      todos.value.push(todo);\n    }\n    const toggleTodo = (index) => {\n      todos.value[index].completed = !todos.value[index].completed;\n    }\n    return{\n      todos,\n      deleteTodo,\n      addTodo,\n      toggleTodo\n    }\n  }\n}\n</script>\n\n<style>\n  .todo{\n    text-decoration: line-through;\n    color: gray;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}