{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos?_page=1&limit=5', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        error.value = err;\n      } // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n\n    };\n\n    const getTodos = async () => {\n      try {\n        const res = await axios.get('http://localhost:3000/todos?_page=1&limit=5');\n        todos.value = res.data;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    getTodos();\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMQ,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMS,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMP,KAAK,CAACQ,IAAN,CAAW,6CAAX,EAAyD;AACzEC,iBAAO,EAAEH,IAAI,CAACG,OAD2D;AAEzEC,mBAAS,EAAEJ,IAAI,CAACI;AAFyD,SAAzD,CAAlB;AAIAP,aAAK,CAACQ,KAAN,CAAYC,IAAZ,CAAiBL,GAAG,CAACM,IAArB;AACD,OAND,CAMC,OAAMC,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF,OAT8B,CAU9B;AACA;AACA;AACA;AACA;;AACD,KAfD;;AAiBA,UAAMC,QAAO,GAAI,YAAW;AAC1B,UAAG;AACD,cAAMR,GAAE,GAAI,MAAMP,KAAK,CAACgB,GAAN,CAAU,6CAAV,CAAlB;AACAb,aAAK,CAACQ,KAAN,GAAcJ,GAAG,CAACM,IAAlB;AACD,OAHD,CAGC,OAAMC,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF;AACF,KAPA;;AAQAC,YAAQ;;AAER,UAAME,UAAS,GAAI,MAAOC,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIhB,KAAK,CAACQ,KAAN,CAAYO,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAMnB,KAAK,CAACoB,KAAN,CAAY,iCAA+BD,EAA3C,EAA8C;AAClDT,mBAAS,EAAE,CAACP,KAAK,CAACQ,KAAN,CAAYO,KAAZ,EAAmBR;AADmB,SAA9C,CAAN;AAGAP,aAAK,CAACQ,KAAN,CAAYO,KAAZ,EAAmBR,SAAnB,GAA+B,CAACP,KAAK,CAACQ,KAAN,CAAYO,KAAZ,EAAmBR,SAAnD;AACD,OALD,CAKC,OAAMI,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF;AAED,KAXD;;AAaA,UAAMO,UAAS,GAAI,MAAOH,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIhB,KAAK,CAACQ,KAAN,CAAYO,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAMnB,KAAK,CAACsB,MAAN,CAAa,iCAA+BH,EAA5C,CAAN;AACAhB,aAAK,CAACQ,KAAN,CAAYY,MAAZ,CAAmBL,KAAnB,EAA0B,CAA1B;AACAd,aAAK,CAACO,KAAN,GAAc,EAAd;AACD,OAJD,CAIC,OAAMG,GAAN,EAAU;AACTV,aAAK,CAACO,KAAN,GAAcG,GAAd;AACF;AACD,KATD;;AAWA,UAAMU,UAAS,GAAI5B,GAAG,CAAC,EAAD,CAAtB;AACA,UAAM6B,aAAY,GAAI5B,QAAQ,CAAC,MAAM;AACnC,UAAG2B,UAAU,CAACb,KAAd,EAAoB;AAClB,eAAOR,KAAK,CAACQ,KAAN,CAAYe,MAAZ,CAAmBpB,IAAG,IAAK;AAChC,iBAAOA,IAAI,CAACG,OAAL,CAAakB,QAAb,CAAsBH,UAAU,CAACb,KAAjC,CAAP;AACD,SAFM,CAAP;AAGD,OAJD,MAIK;AACH,eAAOR,KAAK,CAACQ,KAAb;AACF;AACD,KAR6B,CAA9B;AASA,WAAO;AACLR,WADK;AAELE,aAFK;AAGLgB,gBAHK;AAILJ,gBAJK;AAKLO,gBALK;AAMLC,mBANK;AAOLrB,WAPK;AAQLW;AARK,KAAP;AAUF;;AAhFa,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","axios","components","setup","todos","error","addTodo","todo","res","post","subject","completed","value","push","data","err","getTodos","get","toggleTodo","index","id","patch","deleteTodo","delete","splice","searchText","filteredTodos","filter","includes"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n\n    <h2>To-Do List</h2>\n    <input\n        class=\"form-control\"\n        type=\"text\" \n        v-model=\"searchText\"\n        placeholder=\"search\"\n    >\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    <div style=\"color:red\">{{error}}</div>\n    <div v-if=\"!filteredTodos.length\">\n      There is nothing\n    </div>\n    <TodoList :todos=\"filteredTodos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n\n    <nav aria-label=\"Page navigation example\">\n      <ul class=\"pagination\">\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\">Previous</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\">1</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\">2</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\">3</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\">Next</a></li>\n      </ul>\n    </nav>\n  </div>\n</template>\n\n<script>\nimport { ref, computed} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n\n    const addTodo = async (todo) => {\n      try{\n        const res = await axios.post('http://localhost:3000/todos?_page=1&limit=5',{\n          subject: todo.subject,\n          completed: todo.completed,\n        });\n        todos.value.push(res.data);\n      }catch(err){\n        error.value = err;\n      } \n      // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n    };\n    \n    const getTodos = async() => {\n      try{\n        const res = await axios.get('http://localhost:3000/todos?_page=1&limit=5')\n        todos.value = res.data;\n      }catch(err){\n        error.value = err;\n      }      \n    }\n    getTodos();\n\n    const toggleTodo = async (index) => {\n      const id = todos.value[index].id;\n      try{\n        await axios.patch('http://localhost:3000/todos/'+id,{\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      }catch(err){\n        error.value = err;\n      }\n      \n    };\n\n    const deleteTodo = async (index) => {\n      const id = todos.value[index].id;\n      try{\n        await axios.delete('http://localhost:3000/todos/'+id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      }catch(err){\n        error.value = err;\n      }      \n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if(searchText.value){\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }else{\n        return todos.value;\n      }\n    })\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}