{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = todo => {\n      todos.value.push(todo);\n    };\n\n    const toggleTodo = index => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const count = ref(1);\n    const dCount = computed(() => {\n      return count.value * 2;\n    });\n\n    const dCountMethod = () => {\n      return count.value * 2;\n    };\n\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      count,\n      dCount,\n      dCountMethod\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVF,kBADU;AAEVC;AAFU,GADC;;AAKbE,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIN,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMO,OAAM,GAAKC,IAAD,IAAU;AACxBF,WAAK,CAACG,KAAN,CAAYC,IAAZ,CAAiBF,IAAjB;AACD,KAFD;;AAIA,UAAMG,UAAS,GAAKC,KAAD,IAAW;AAC5BN,WAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBC,SAAnB,GAA+B,CAACP,KAAK,CAACG,KAAN,CAAYG,KAAZ,EAAmBC,SAAnD;AACD,KAFD;;AAIA,UAAMC,UAAS,GAAKF,KAAD,IAAW;AAC5BN,WAAK,CAACG,KAAN,CAAYM,MAAZ,CAAmBH,KAAnB,EAA0B,CAA1B;AACD,KAFD;;AAIA,UAAMI,KAAI,GAAIhB,GAAG,CAAC,CAAD,CAAjB;AACA,UAAMiB,MAAK,GAAIhB,QAAQ,CAAC,MAAM;AAC5B,aAAOe,KAAK,CAACP,KAAN,GAAc,CAArB;AACD,KAFsB,CAAvB;;AAGA,UAAMS,YAAW,GAAI,MAAM;AACzB,aAAOF,KAAK,CAACP,KAAN,GAAc,CAArB;AACF,KAFA;;AAGA,WAAO;AACLH,WADK;AAELC,aAFK;AAGLO,gBAHK;AAILH,gBAJK;AAKLK,WALK;AAMLC,YANK;AAOLC;AAPK,KAAP;AASF;;AApCa,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","components","setup","todos","addTodo","todo","value","push","toggleTodo","index","completed","deleteTodo","splice","count","dCount","dCountMethod"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h3>count: {{count}}</h3>\n    <h3>dCount: {{dCount}}</h3>\n    <h3>dCountMethod: {{dCountMethod()}}</h3>\n    <button @click=\"count++\">add</button>\n    <h2>To-Do List</h2>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    \n    <div v-if=\"!todos.length\">\n      추가된 Todo가 없습니다\n    </div>\n    <TodoList :todos=\"todos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport { ref, computed} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList,\n  },\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = (todo) => {\n      todos.value.push(todo);\n    };\n\n    const toggleTodo = (index) => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index, 1);\n    };\n\n    const count = ref(1);\n    const dCount = computed(() => {\n      return count.value * 2;\n    });\n    const dCountMethod = () => {\n      return count.value * 2;\n    }\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      count,\n      dCount,\n      dCountMethod\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}