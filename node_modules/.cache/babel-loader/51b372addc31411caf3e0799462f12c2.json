{"ast":null,"code":"export default {\n  props: {\n    todos: {\n      type: Array,\n      required: true\n    }\n  },\n  emits: ['toggle-todo', 'delete-todo'],\n\n  setup(props, {\n    emit\n  }) {\n    const toggleTodo = index => {\n      emit('toggle-todo', index);\n    };\n\n    const deleteTodo = index => {\n      emit('delete-todo', index);\n    };\n\n    return {\n      toggleTodo,\n      deleteTodo\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAkCA,eAAe;AACXA,OAAK,EAAE;AACHC,SAAK,EAAE;AACHC,UAAI,EAAEC,KADH;AAEHC,cAAQ,EAAE;AAFP;AADJ,GADI;AAOXC,OAAK,EAAE,CAAC,aAAD,EAAe,aAAf,CAPI;;AAQXC,OAAK,CAACN,KAAD,EAAQ;AAACO;AAAD,GAAR,EAAgB;AACnB,UAAMC,UAAS,GAAKC,KAAD,IAAW;AAC5BF,UAAI,CAAC,aAAD,EAAgBE,KAAhB,CAAJ;AACD,KAFD;;AAGA,UAAMC,UAAS,GAAKD,KAAD,IAAW;AAC5BF,UAAI,CAAC,aAAD,EAAgBE,KAAhB,CAAJ;AACF,KAFA;;AAIA,WAAO;AACLD,gBADK;AAELE;AAFK,KAAP;AAIF;;AApBW,CAAf","names":["props","todos","type","Array","required","emits","setup","emit","toggleTodo","index","deleteTodo"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/components/TodoList.vue"],"sourcesContent":["<template>\n  <div \n      v-for=\"(todo, index) in todos\"\n      :key=\"todo.id\"\n      class=\"card mt-2\"\n    >\n      <div class=\"card-body p-2 d-flex align-items-center\">\n        <div class=\"form-check flex-grow-1\">\n          <input \n            class=\"form-check-input\" \n            type=\"checkbox\"\n            :value=\"todo.completed\"\n            @change=\"toggleTodo(index)\"\n          >\n          <label \n            class=\"form-check-label\"\n            :class=\"{ todo: todo.completed }\"\n          >\n            {{ todo.subject }}\n          </label>\n        </div>\n        <div>\n          <button \n            class=\"btn btn-danger btn-sm\"\n            @click=\"deleteTodo(index)\"\n          >\n            X\n          </button>\n        </div>\n      </div>\n    </div>\n</template>\n\n<script>\nexport default {\n    props: {\n        todos: {\n            type: Array,\n            required: true\n        }\n    },\n    emits: ['toggle-todo','delete-todo'],\n    setup(props, {emit}) {\n      const toggleTodo = (index) => {\n        emit('toggle-todo', index);\n      };\n      const deleteTodo = (index) => {\n        emit('delete-todo', index);\n      }\n\n      return {\n        toggleTodo,\n        deleteTodo\n      }\n    }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}