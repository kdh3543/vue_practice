{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n\n    const addTodo = todo => {\n      axios.post('http://localhost:3000/todos', {\n        subject: todo.subject,\n        completed: todo.completed\n      }).then(res => {\n        todos.value.push(res.data);\n      }).catch(err => {\n        error.value = err;\n      });\n    };\n\n    const toggleTodo = index => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMQ,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMS,OAAM,GAAKC,IAAD,IAAU;AACxBN,WAAK,CAACO,IAAN,CAAW,6BAAX,EAAyC;AACvCC,eAAO,EAAEF,IAAI,CAACE,OADyB;AAEvCC,iBAAS,EAAEH,IAAI,CAACG;AAFuB,OAAzC,EAGGC,IAHH,CAGQC,GAAE,IAAK;AACbR,aAAK,CAACS,KAAN,CAAYC,IAAZ,CAAiBF,GAAG,CAACG,IAArB;AACD,OALD,EAKGC,KALH,CAKSC,GAAE,IAAK;AACdZ,aAAK,CAACQ,KAAN,GAAcI,GAAd;AACD,OAPD;AAQD,KATD;;AAWA,UAAMC,UAAS,GAAKC,KAAD,IAAW;AAC5Bf,WAAK,CAACS,KAAN,CAAYM,KAAZ,EAAmBT,SAAnB,GAA+B,CAACN,KAAK,CAACS,KAAN,CAAYM,KAAZ,EAAmBT,SAAnD;AACD,KAFD;;AAIA,UAAMU,UAAS,GAAKD,KAAD,IAAW;AAC5Bf,WAAK,CAACS,KAAN,CAAYQ,MAAZ,CAAmBF,KAAnB,EAA0B,CAA1B;AACD,KAFD;;AAIA,UAAMG,UAAS,GAAIzB,GAAG,CAAC,EAAD,CAAtB;AACA,UAAM0B,aAAY,GAAIzB,QAAQ,CAAC,MAAM;AACnC,UAAGwB,UAAU,CAACT,KAAd,EAAoB;AAClB,eAAOT,KAAK,CAACS,KAAN,CAAYW,MAAZ,CAAmBjB,IAAG,IAAK;AAChC,iBAAOA,IAAI,CAACE,OAAL,CAAagB,QAAb,CAAsBH,UAAU,CAACT,KAAjC,CAAP;AACD,SAFM,CAAP;AAGD,OAJD,MAIK;AACH,eAAOT,KAAK,CAACS,KAAb;AACF;AACD,KAR6B,CAA9B;AASA,WAAO;AACLT,WADK;AAELE,aAFK;AAGLc,gBAHK;AAILF,gBAJK;AAKLI,gBALK;AAMLC,mBANK;AAOLlB;AAPK,KAAP;AASF;;AA/Ca,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","axios","components","setup","todos","error","addTodo","todo","post","subject","completed","then","res","value","push","data","catch","err","toggleTodo","index","deleteTodo","splice","searchText","filteredTodos","filter","includes"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n\n    <h2>To-Do List</h2>\n    <input\n        class=\"form-control\"\n        type=\"text\" \n        v-model=\"searchText\"\n        placeholder=\"search\"\n    >\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    <div style=\"color:red\">{{error}}</div>\n    <div v-if=\"!filteredTodos.length\">\n      There is nothing\n    </div>\n    <TodoList :todos=\"filteredTodos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport { ref, computed} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n\n    const addTodo = (todo) => {\n      axios.post('http://localhost:3000/todos',{\n        subject: todo.subject,\n        completed: todo.completed,\n      }).then(res => {\n        todos.value.push(res.data);\n      }).catch(err => {\n        error.value = err;\n      });\n    };\n\n    const toggleTodo = (index) => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if(searchText.value){\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }else{\n        return todos.value;\n      }\n    })\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}