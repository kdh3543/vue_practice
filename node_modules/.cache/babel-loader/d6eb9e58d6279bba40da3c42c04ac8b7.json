   <input type=\"checkbox\" class=\"form-check-input\">\n          <label class=\"form-check-label\">{{list.contents}}</label>\n        </div>\n        \n      </div>\n    </div>\n  </div>\n  \n</template>\n\n<script>\nimport {ref} from 'vue';\n\nexport default {\n  setup(){\n    const hasError = ref(false);\n    const todo = ref('');\n    const news = ref([]);\n    const onSubmit = () => {\n      if(todo.value){\n        hasError.value = false;\n        news.value.push({\n          id:Date.now(),\n          contents: todo.value\n        })\n        todo.value='';\n      }else{\n        hasError.value = true;\n      }\n    }\n    \n    return{\n      todo,\n      onSubmit,\n      news,\n      hasError\n    }\n  }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}