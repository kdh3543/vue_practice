{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport Modal from '@/components/Modal.vue';\nexport default {\n  components: {\n    Modal\n  },\n  props: {\n    todos: {\n      type: Array,\n      required: true\n    }\n  },\n  emits: ['toggle-todo', 'delete-todo'],\n\n  setup(props, {\n    emit\n  }) {\n    const router = useRouter();\n\n    const toggleTodo = (index, event) => {\n      emit('toggle-todo', index, event.target.checked);\n    };\n\n    const deleteTodo = index => {\n      emit('delete-todo', index);\n    };\n\n    const moveToPage = todoId => {\n      console.log(todoId);\n      router.push({\n        name: 'Todo',\n        params: {\n          id: todoId\n        }\n      });\n    };\n\n    return {\n      toggleTodo,\n      deleteTodo,\n      moveToPage,\n      router\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqCA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,wBAAlB;AAEA,eAAe;AACXC,YAAU,EAAC;AACTD;AADS,GADA;AAIXE,OAAK,EAAE;AACHC,SAAK,EAAE;AACHC,UAAI,EAAEC,KADH;AAEHC,cAAQ,EAAE;AAFP;AADJ,GAJI;AAUXC,OAAK,EAAE,CAAC,aAAD,EAAe,aAAf,CAVI;;AAWXC,OAAK,CAACN,KAAD,EAAQ;AAACO;AAAD,GAAR,EAAgB;AACnB,UAAMC,MAAK,GAAIX,SAAS,EAAxB;;AACA,UAAMY,UAAS,GAAI,CAACC,KAAD,EAAOC,KAAP,KAAiB;AAClCJ,UAAI,CAAC,aAAD,EAAgBG,KAAhB,EAAsBC,KAAK,CAACC,MAAN,CAAaC,OAAnC,CAAJ;AACD,KAFD;;AAGA,UAAMC,UAAS,GAAKJ,KAAD,IAAW;AAC5BH,UAAI,CAAC,aAAD,EAAgBG,KAAhB,CAAJ;AACF,KAFA;;AAIA,UAAMK,UAAS,GAAKC,MAAD,IAAY;AAC7BC,aAAO,CAACC,GAAR,CAAYF,MAAZ;AACAR,YAAM,CAACW,IAAP,CAAY;AACVC,YAAI,EAAE,MADI;AAEVC,cAAM,EAAC;AACLC,YAAE,EAAEN;AADC;AAFG,OAAZ;AAMF,KARA;;AAUA,WAAO;AACLP,gBADK;AAELK,gBAFK;AAGLC,gBAHK;AAILP;AAJK,KAAP;AAMF;;AApCW,CAAf","names":["useRouter","Modal","components","props","todos","type","Array","required","emits","setup","emit","router","toggleTodo","index","event","target","checked","deleteTodo","moveToPage","todoId","console","log","push","name","params","id"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/components/TodoList.vue"],"sourcesContent":["<template>\n<div>\n  <div v-for=\"(todo, index) in todos\" :key=\"todo.id\" class=\"card mt-2\">\n    <div class=\"card-body p-2 d-flex align-items-center\" >\n      <div class=\"form-check flex-grow-1\">\n        <input \n          class=\"form-check-input\" \n          type=\"checkbox\"\n          :checked=\"todo.completed\"\n          @click.stop=\"toggleTodo(index,$event)\"\n          style=\"cursor:pointer\"\n        >\n        <label \n          class=\"form-check-label\"\n          id=\"label\"\n          :class=\"{ todo: todo.completed }\"\n          @click=\"moveToPage(todo.id)\"\n        >\n          {{ todo.subject }}\n        </label>\n      </div>\n      <div>\n        <button \n          class=\"btn btn-danger btn-sm\"\n          @click.stop=\"deleteTodo(index)\"\n        >\n          X\n        </button>\n      </div>\n    </div>\n  </div>\n  <Modal/>\n</div>\n  \n</template>\n\n<script>\nimport { useRouter } from 'vue-router';\nimport Modal from '@/components/Modal.vue';\n\nexport default {\n    components:{\n      Modal\n    },\n    props: {\n        todos: {\n            type: Array,\n            required: true\n        }\n    },\n    emits: ['toggle-todo','delete-todo'],\n    setup(props, {emit}) {\n      const router = useRouter();\n      const toggleTodo = (index,event) => {\n        emit('toggle-todo', index,event.target.checked);\n      };\n      const deleteTodo = (index) => {\n        emit('delete-todo', index);\n      }\n\n      const moveToPage = (todoId) => {\n        console.log(todoId);\n        router.push({\n          name: 'Todo',\n          params:{\n            id: todoId\n          }\n        });\n      }\n\n      return {\n        toggleTodo,\n        deleteTodo,\n        moveToPage,\n        router\n      }\n    }\n}\n</script>\n\n<style>\n  #label:hover{\n    cursor: pointer;\n    transform: scale(1.4);\n  }\n</style>"]},"metadata":{},"sourceType":"module"}