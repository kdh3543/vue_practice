{"ast":null,"code":"import { ref } from 'vue';\nimport { useRoute } from 'vue-router';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const route = useRoute();\n    const todo = ref(null);\n    const loading = ref(true);\n\n    const getTodo = async () => {\n      const res = await axios.get('http://localhost:3000/todos/' + route.params.id);\n      todo.value = res.data;\n      loading.value = false;\n    };\n\n    getTodo();\n    return {\n      todo,\n      loading\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAiBA,SAAQA,GAAR,QAAkB,KAAlB;AACA,SAAQC,QAAR,QAAuB,YAAvB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACXC,OAAK,GAAE;AACH,UAAMC,KAAI,GAAIH,QAAQ,EAAtB;AACA,UAAMI,IAAG,GAAIL,GAAG,CAAC,IAAD,CAAhB;AACA,UAAMM,OAAM,GAAIN,GAAG,CAAC,IAAD,CAAnB;;AAEA,UAAMO,OAAM,GAAI,YAAY;AAC1B,YAAMC,GAAE,GAAI,MAAMN,KAAK,CAACO,GAAN,CAAU,iCAA+BL,KAAK,CAACM,MAAN,CAAaC,EAAtD,CAAlB;AAEAN,UAAI,CAACO,KAAL,GAAaJ,GAAG,CAACK,IAAjB;AACAP,aAAO,CAACM,KAAR,GAAgB,KAAhB;AACF,KALA;;AAOAL,WAAO;AACP,WAAM;AACJF,UADI;AAEJC;AAFI,KAAN;AAIJ;;AAlBW,CAAf","names":["ref","useRoute","axios","setup","route","todo","loading","getTodo","res","get","params","id","value","data"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/pages/todos/_id.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>To-Do Page</h1>\n    <div v-if=\"loading\">\n      Loading...\n    </div>\n    <form v-else>\n      <div class=\"form-group\">\n        <label>Todo Subject</label>\n        <input v-model=\"todo.subject\" type=\"text\" class=\"form-control\">\n      </div>\n      <button class=\"btn btn-primary\">Save</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport {useRoute} from 'vue-router';\nimport axios from 'axios';\n\nexport default {\n    setup(){\n        const route = useRoute();\n        const todo = ref(null);\n        const loading = ref(true);\n\n        const getTodo = async () => {\n          const res = await axios.get('http://localhost:3000/todos/'+route.params.id)\n          \n          todo.value = res.data;\n          loading.value = false;\n        }\n        \n        getTodo();\n        return{\n          todo,\n          loading\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}