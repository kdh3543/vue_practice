{"ast":null,"code":"import { ref } from 'vue';\nexport default {\n  setup() {\n    const todo = ref('');\n    const todos = ref([]);\n\n    const onSubmit = () => {\n      todos.value.push({\n        id: Date.now(),\n        contents: todo.value,\n        completed: false\n      });\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    return {\n      todo,\n      todos,\n      onSubmit,\n      deleteTodo\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,SAAQA,GAAR,QAAkB,KAAlB;AAEA,eAAe;AACbC,OAAK,GAAE;AACL,UAAMC,IAAG,GAAIF,GAAG,CAAC,EAAD,CAAhB;AACA,UAAMG,KAAI,GAAIH,GAAG,CAAC,EAAD,CAAjB;;AACA,UAAMI,QAAO,GAAI,MAAM;AACrBD,WAAK,CAACE,KAAN,CAAYC,IAAZ,CAAiB;AACfC,UAAE,EAAEC,IAAI,CAACC,GAAL,EADW;AAEfC,gBAAQ,EAAER,IAAI,CAACG,KAFA;AAGfM,iBAAS,EAAE;AAHI,OAAjB;AAKF,KANA;;AAOA,UAAMC,UAAS,GAAKC,KAAD,IAAW;AAC5BV,WAAK,CAACE,KAAN,CAAYS,MAAZ,CAAmBD,KAAnB,EAAyB,CAAzB;AACF,KAFA;;AAGA,WAAM;AACJX,UADI;AAEJC,WAFI;AAGJC,cAHI;AAIJQ;AAJI,KAAN;AAMF;;AApBa,CAAf","names":["ref","setup","todo","todos","onSubmit","value","push","id","Date","now","contents","completed","deleteTodo","index","splice"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>To-Do List</h2>\n    <hr>\n    <form @submit.prevent=\"onSubmit\" class=\"d-flex\">\n      <div class=\"flex-grow-1\">\n        <input class=\"form-control\" type=\"text\" placeholder=\"write new to-do\" v-model=\"todo\"> \n      </div>\n      <div>\n        <button class=\"btn btn-primary\">Add</button>\n      </div>\n      \n    </form>\n    <hr>\n    <div class=\"card mt-1\" v-for=\"(list,index) in todos\" :key=\"index\">\n      <div class=\"card-body p-2 check-form d-flex align-items-center\">\n        <div class=\"flex-grow-1\">\n          <input type=\"checkbox\" class=\"check-form-input\" v-model=\"list.completed\">\n          <label class=\"check-form-label\" :class=\"{todo:list.completed}\">{{list.contents}}</label>\n        </div>\n        <div>\n          <button class=\"btn btn-danger btn-sm\" @click=\"deleteTodo(index)\">X</button>\n        </div>\n      </div>\n      \n    </div>\n   \n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\n\nexport default {\n  setup(){\n    const todo = ref('');\n    const todos = ref([]);\n    const onSubmit = () => {\n      todos.value.push({\n        id: Date.now(),\n        contents: todo.value,\n        completed: false,\n      })\n    }\n    const deleteTodo = (index) => {\n      todos.value.splice(index,1);\n    }\n    return{\n      todo,\n      todos,\n      onSubmit,\n      deleteTodo\n    }\n  }\n}\n</script>\n\n<style>\n  .todo{\n    text-decoration: line-through;\n    color: red;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}