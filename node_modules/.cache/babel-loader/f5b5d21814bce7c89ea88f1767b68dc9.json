{"ast":null,"code":"import { ref, computed, watch } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n    const numberOfTodos = ref(0);\n    const currentPage = ref(1);\n    const limit = 5;\n    const numberOfPages = computed(() => {\n      return Math.ceil(numberOfTodos.value / limit);\n    });\n    watch(currentPage, () => {\n      console.log('hello');\n    });\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos?_page=1&_limit=5', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        error.value = err;\n      } // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n\n    };\n\n    const getTodos = async (page = currentPage.value) => {\n      currentPage.value = page;\n\n      try {\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`);\n        numberOfTodos.value = res.headers['x-total-count'];\n        todos.value = res.data;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    getTodos();\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const searchText = ref(''); // const filteredTodos = computed(() => {\n    //   if(searchText.value){\n    //     return todos.value.filter(todo => {\n    //       return todo.subject.includes(searchText.value);\n    //     });\n    //   }else{\n    //     return todos.value;\n    //   }\n    // })\n\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      // filteredTodos,\n      error,\n      getTodos,\n      numberOfPages,\n      currentPage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAT,EAAcC,QAAd,EAAwBC,KAAxB,QAAoC,KAApC;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMS,KAAI,GAAIT,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMU,aAAY,GAAIV,GAAG,CAAC,CAAD,CAAzB;AACA,UAAMW,WAAU,GAAIX,GAAG,CAAC,CAAD,CAAvB;AACA,UAAMY,KAAI,GAAI,CAAd;AACA,UAAMC,aAAY,GAAIZ,QAAQ,CAAC,MAAM;AACnC,aAAOa,IAAI,CAACC,IAAL,CAAUL,aAAa,CAACM,KAAd,GAAoBJ,KAA9B,CAAP;AACD,KAF6B,CAA9B;AAIAV,SAAK,CAACS,WAAD,EAAc,MAAM;AACvBM,aAAO,CAACC,GAAR,CAAY,OAAZ;AACD,KAFI,CAAL;;AAIA,UAAMC,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMhB,KAAK,CAACiB,IAAN,CAAW,8CAAX,EAA0D;AAC1EC,iBAAO,EAAEH,IAAI,CAACG,OAD4D;AAE1EC,mBAAS,EAAEJ,IAAI,CAACI;AAF0D,SAA1D,CAAlB;AAIAhB,aAAK,CAACQ,KAAN,CAAYS,IAAZ,CAAiBJ,GAAG,CAACK,IAArB;AACD,OAND,CAMC,OAAMC,GAAN,EAAU;AACTlB,aAAK,CAACO,KAAN,GAAcW,GAAd;AACF,OAT8B,CAU9B;AACA;AACA;AACA;AACA;;AACD,KAfD;;AAiBA,UAAMC,QAAO,GAAI,OAAMC,IAAG,GAAIlB,WAAW,CAACK,KAAzB,KAAmC;AAClDL,iBAAW,CAACK,KAAZ,GAAoBa,IAApB;;AACA,UAAG;AACD,cAAMR,GAAE,GAAI,MAAMhB,KAAK,CAACyB,GAAN,CAAW,qCAAoCD,IAAK,WAAUjB,KAAM,EAApE,CAAlB;AACAF,qBAAa,CAACM,KAAd,GAAsBK,GAAG,CAACU,OAAJ,CAAY,eAAZ,CAAtB;AACAvB,aAAK,CAACQ,KAAN,GAAcK,GAAG,CAACK,IAAlB;AACD,OAJD,CAIC,OAAMC,GAAN,EAAU;AACTl