{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n    const numberOfTodos = ref(0);\n    const page = ref(1);\n    const limit = 5;\n    const numberOfPages = computed(() => {\n      return Math.ceil(numberOfTodos / limit);\n    });\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos?_page=1&_limit=5', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        error.value = err;\n      } // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n\n    };\n\n    const getTodos = async () => {\n      try {\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page.value}&_limit=${limit}`);\n        numberOfTodos.value = res.headers['x-total-count'];\n        todos.value = res.data;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    getTodos();\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA+BA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMQ,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMS,aAAY,GAAIT,GAAG,CAAC,CAAD,CAAzB;AACA,UAAMU,IAAG,GAAIV,GAAG,CAAC,CAAD,CAAhB;AACA,UAAMW,KAAI,GAAI,CAAd;AACA,UAAMC,aAAY,GAAIX,QAAQ,CAAC,MAAM;AACnC,aAAOY,IAAI,CAACC,IAAL,CAAUL,aAAa,GAACE,KAAxB,CAAP;AACD,KAF6B,CAA9B;;AAIA,UAAMI,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMb,KAAK,CAACc,IAAN,CAAW,8CAAX,EAA0D;AAC1EC,iBAAO,EAAEH,IAAI,CAACG,OAD4D;AAE1EC,mBAAS,EAAEJ,IAAI,CAACI;AAF0D,SAA1D,CAAlB;AAIAb,aAAK,CAACc,KAAN,CAAYC,IAAZ,CAAiBL,GAAG,CAACM,IAArB;AACD,OAND,CAMC,OAAMC,GAAN,EAAU;AACThB,aAAK,CAACa,KAAN,GAAcG,GAAd;AACF,OAT8B,CAU9B;AACA;AACA;AACA;AACA;;AACD,KAfD;;AAiBA,UAAMC,QAAO,GAAI,YAAW;AAC1B,UAAG;AACD,cAAMR,GAAE,GAAI,MAAMb,KAAK,CAACsB,GAAN,CAAW,qCAAoChB,IAAI,CAACW,KAAM,WAAUV,KAAM,EAA1E,CAAlB;AACAF,qBAAa,CAACY,KAAd,GAAsBJ,GAAG,CAACU,OAAJ,CAAY,eAAZ,CAAtB;AACApB,aAAK,CAACc,KAAN,GAAcJ,GAAG,CAACM,IAAlB;AACD,OAJD,CAIC,OAAMC,GAAN,EAAU;AACThB,aAAK,CAACa,KAAN,GAAcG,GAAd;AACF;AACF,KARA;;AASAC,YAAQ;;AAER,UAAMG,UAAS,GAAI,MAAOC,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIvB,KAAK,CAACc,KAAN,CAAYQ,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAM1B,KAAK,CAAC2B,KAAN,CAAY,iCAA+BD,EAA3C,EAA8C;AAClDV,mBAAS,EAAE,CAACb,KAAK,CAACc,KAAN,CAAYQ,KAAZ,EAAmBT;AADmB,SAA9C,CAAN;AAGAb,aAAK,CAACc,KAAN,CAAYQ,KAAZ,EAAmBT,SAAnB,GAA+B,CAACb,KAAK,CAACc,KAAN,CAAYQ