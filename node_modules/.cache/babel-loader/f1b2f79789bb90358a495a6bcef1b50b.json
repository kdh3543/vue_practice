{"ast":null,"code":"import { ref } from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\nexport default {\n  components: {\n    SimpleForm\n  },\n\n  setup() {\n    // const completed = ref(false);\n    const hasError = ref(false);\n    const todo = ref('');\n    const todos = ref([]);\n\n    const onSubmit = () => {\n      if (todo.value) {\n        hasError.value = false;\n        todos.value.push({\n          id: Date.now(),\n          contents: todo.value,\n          completed: false\n        });\n        todo.value = \"\";\n      } else {\n        hasError.value = true;\n      }\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    return {\n      onSubmit,\n      todo,\n      todos,\n      hasError,\n      deleteTodo\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA2BA,SAAQA,GAAR,QAAkB,KAAlB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVD;AADU,GADC;;AAIbE,OAAK,GAAE;AACL;AACA,UAAMC,QAAO,GAAIJ,GAAG,CAAC,KAAD,CAApB;AACA,UAAMK,IAAG,GAAIL,GAAG,CAAC,EAAD,CAAhB;AACA,UAAMM,KAAI,GAAIN,GAAG,CAAC,EAAD,CAAjB;;AACA,UAAMO,QAAO,GAAI,MAAM;AACrB,UAAGF,IAAI,CAACG,KAAR,EAAc;AACZJ,gBAAQ,CAACI,KAAT,GAAe,KAAf;AACAF,aAAK,CAACE,KAAN,CAAYC,IAAZ,CAAiB;AAEfC,YAAE,EAAEC,IAAI,CAACC,GAAL,EAFW;AAGfC,kBAAQ,EAAER,IAAI,CAACG,KAHA;AAIfM,mBAAS,EAAE;AAJI,SAAjB;AAMAT,YAAI,CAACG,KAAL,GAAW,EAAX;AACD,OATD,MASK;AACHJ,gBAAQ,CAACI,KAAT,GAAe,IAAf;AACF;AACF,KAbA;;AAeA,UAAMO,UAAS,GAAKC,KAAD,IAAW;AAC5BV,WAAK,CAACE,KAAN,CAAYS,MAAZ,CAAmBD,KAAnB,EAAyB,CAAzB;AACF,KAFA;;AAIA,WAAM;AACJT,cADI;AAEJF,UAFI;AAGJC,WAHI;AAIJF,cAJI;AAKJW;AALI,KAAN;AAQF;;AApCa,CAAf","names":["ref","SimpleForm","components","setup","hasError","todo","todos","onSubmit","value","push","id","Date","now","contents","completed","deleteTodo","index","splice"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h2>To-Do List</h2>\n    <SimpleForm/>\n    <div v-if=\"hasError\">\n      Error\n    </div>\n    <div v-if=\"!todos.length\">\n      nothing\n    </div>\n    <div class=\"card mt-2\" v-for=\"(list,index) in todos\" :key=\"index\">\n      <div class=\"card-body p-2 d-flex align-items-center\">\n        <div class=\"form-check flex-grow-1 \">\n          <input class=\"form-check-input\" type=\"checkbox\" v-model=\"list.completed\">\n          <label class=\"form-check-label\" :class=\"{todo:list.completed}\"> {{list.contents}}</label>\n        </div>\n        <div>\n          <button class=\"btn btn-danger btn-sm\" @click=\"deleteTodo(index)\">X</button>\n        </div>\n      </div>\n      \n    </div>\n    \n  </div>\n</template>\n\n<script>\nimport {ref} from 'vue';\nimport SimpleForm from './components/SimpleForm.vue';\n\nexport default {\n  components: {\n    SimpleForm\n  },\n  setup(){\n    // const completed = ref(false);\n    const hasError = ref(false);\n    const todo = ref('');\n    const todos = ref([]);\n    const onSubmit = () => {\n      if(todo.value){\n        hasError.value=false\n        todos.value.push({\n          \n          id: Date.now(),\n          contents: todo.value,\n          completed: false,\n        })\n        todo.value=\"\";\n      }else{\n        hasError.value=true\n      }\n    }\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index,1);\n    }\n\n    return{\n      onSubmit,\n      todo,\n      todos,\n      hasError,\n      deleteTodo\n      \n    }\n  }\n}\n</script>\n\n<style>\n  .todo{\n    text-decoration: line-through;\n    color: gray;\n  }\n</style>\n"]},"metadata":{},"sourceType":"module"}