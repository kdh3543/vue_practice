{"ast":null,"code":"import { useRouter } from 'vue-router';\nimport Modal from '@/components/DeleteModal.vue';\nimport { ref } from 'vue';\nimport List from '@/components/List.vue';\nexport default {\n  components: {\n    Modal,\n    List\n  },\n  props: {\n    todos: {\n      type: Array,\n      required: true\n    }\n  },\n  emits: ['toggle-todo', 'delete-todo'],\n\n  setup(props, {\n    emit\n  }) {\n    const showModal = ref(false);\n    const todoDeleteId = ref(null);\n    const router = useRouter();\n\n    const toggleTodo = (index, event) => {\n      emit('toggle-todo', index, event.target.checked);\n    };\n\n    const openModal = id => {\n      todoDeleteId.value = id;\n      showModal.value = true;\n    };\n\n    const closeModal = id => {\n      todoDeleteId.value = null;\n      showModal.value = false;\n    };\n\n    const deleteTodo = () => {\n      emit('delete-todo', todoDeleteId.value);\n      showModal.value = false;\n      todoDeleteId.value = null;\n    };\n\n    const moveToPage = todoId => {\n      console.log(todoId);\n      router.push({\n        name: 'Todo',\n        params: {\n          id: todoId\n        }\n      });\n    };\n\n    return {\n      toggleTodo,\n      deleteTodo,\n      moveToPage,\n      router,\n      showModal,\n      openModal,\n      closeModal\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAoDA,SAASA,SAAT,QAA0B,YAA1B;AACA,OAAOC,KAAP,MAAkB,8BAAlB;AACA,SAAQC,GAAR,QAAkB,KAAlB;AACA,OAAOC,IAAP,MAAiB,uBAAjB;AAEA,eAAe;AACXC,YAAU,EAAC;AACTH,SADS;AAETE;AAFS,GADA;AAKXE,OAAK,EAAE;AACHC,SAAK,EAAE;AACHC,UAAI,EAAEC,KADH;AAEHC,cAAQ,EAAE;AAFP;AADJ,GALI;AAWXC,OAAK,EAAE,CAAC,aAAD,EAAe,aAAf,CAXI;;AAYXC,OAAK,CAACN,KAAD,EAAQ;AAACO;AAAD,GAAR,EAAgB;AACnB,UAAMC,SAAQ,GAAIX,GAAG,CAAC,KAAD,CAArB;AACA,UAAMY,YAAW,GAAIZ,GAAG,CAAC,IAAD,CAAxB;AACA,UAAMa,MAAK,GAAIf,SAAS,EAAxB;;AACA,UAAMgB,UAAS,GAAI,CAACC,KAAD,EAAOC,KAAP,KAAiB;AAClCN,UAAI,CAAC,aAAD,EAAgBK,KAAhB,EAAsBC,KAAK,CAACC,MAAN,CAAaC,OAAnC,CAAJ;AACD,KAFD;;AAIA,UAAMC,SAAQ,GAAKC,EAAD,IAAQ;AACxBR,kBAAY,CAACS,KAAb,GAAqBD,EAArB;AACAT,eAAS,CAACU,KAAV,GAAkB,IAAlB;AACF,KAHA;;AAKA,UAAMC,UAAS,GAAKF,EAAD,IAAQ;AACzBR,kBAAY,CAACS,KAAb,GAAqB,IAArB;AACAV,eAAS,CAACU,KAAV,GAAkB,KAAlB;AACF,KAHA;;AAKA,UAAME,UAAS,GAAI,MAAM;AACvBb,UAAI,CAAC,aAAD,EAAgBE,YAAY,CAACS,KAA7B,CAAJ;AACAV,eAAS,CAACU,KAAV,GAAkB,KAAlB;AACAT,kBAAY,CAACS,KAAb,GAAqB,IAArB;AACF,KAJA;;AAMA,UAAMG,UAAS,GAAKC,MAAD,IAAY;AAC7BC,aAAO,CAACC,GAAR,CAAYF,MAAZ;AACAZ,YAAM,CAACe,IAAP,CAAY;AACVC,YAAI,EAAE,MADI;AAEVC,cAAM,EAAC;AACLV,YAAE,EAAEK;AADC;AAFG,OAAZ;AAMF,KARA;;AAUA,WAAO;AACLX,gBADK;AAELS,gBAFK;AAGLC,gBAHK;AAILX,YAJK;AAKLF,eALK;AAMLQ,eANK;AAOLG;AAPK,KAAP;AASF;;AAvDW,CAAf","names":["useRouter","Modal","ref","List","components","props","todos","type","Array","required","emits","setup","emit","showModal","todoDeleteId","router","toggleTodo","index","event","target","checked","openModal","id","value","closeModal","deleteTodo","moveToPage","todoId","console","log","push","name","params"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/components/TodoList.vue"],"sourcesContent":["<template>\n<div>\n  <!-- <div v-for=\"(todo, index) in todos\" :key=\"todo.id\" class=\"card mt-2\"> -->\n    <List :items=\"todos\">\n    <template #default=\"{item, index}\">\n      <div class=\"card-body p-2 d-flex align-items-center\" >\n        <div class=\"form-check flex-grow-1\">\n          <input \n            class=\"form-check-input\" \n            type=\"checkbox\"\n            :checked=\"item.completed\"\n            @click.stop=\"toggleTodo(index,$event)\"\n            style=\"cursor:pointer\"\n          >\n          <label \n            class=\"form-check-label\"\n            id=\"label\"\n            :class=\"{ todo: item.completed }\"\n            @click=\"moveToPage(item.id)\"\n          >\n            {{ item.subject }}\n          </label>\n        </div>\n        <div>\n          <button \n            class=\"btn btn-danger btn-sm\"\n            @click.stop=\"openModal(item.id)\"\n          >\n            X\n          </button>\n        </div>\n      </div>\n    </template>\n  </List>\n  <teleport to=#modal>\n    <Modal v-if=\"showModal\" @close=\"closeModal\" @delete=\"deleteTodo\">\n      <template v-slot:title>\n        Delete Todo!!\n      </template>\n      <template v-slot:body>\n        sure delete?\n      </template>\n      <template v-slot:footer>\n        \n      </template>\n    </Modal>\n  </teleport>\n</div>\n  \n</template>\n\n<script>\nimport { useRouter } from 'vue-router';\nimport Modal from '@/components/DeleteModal.vue';\nimport {ref} from 'vue';\nimport List from '@/components/List.vue';\n\nexport default {\n    components:{\n      Modal,\n      List\n    },\n    props: {\n        todos: {\n            type: Array,\n            required: true\n        }\n    },\n    emits: ['toggle-todo','delete-todo'],\n    setup(props, {emit}) {\n      const showModal = ref(false);\n      const todoDeleteId = ref(null);\n      const router = useRouter();\n      const toggleTodo = (index,event) => {\n        emit('toggle-todo', index,event.target.checked);\n      };\n\n      const openModal = (id) => {\n        todoDeleteId.value = id;\n        showModal.value = true;\n      }\n\n      const closeModal = (id) => {\n        todoDeleteId.value = null;\n        showModal.value = false;\n      }\n\n      const deleteTodo = () => {\n        emit('delete-todo', todoDeleteId.value);\n        showModal.value = false;\n        todoDeleteId.value = null;\n      }\n\n      const moveToPage = (todoId) => {\n        console.log(todoId);\n        router.push({\n          name: 'Todo',\n          params:{\n            id: todoId\n          }\n        });\n      }\n\n      return {\n        toggleTodo,\n        deleteTodo,\n        moveToPage,\n        router,\n        showModal,\n        openModal,\n        closeModal\n      }\n    }\n}\n</script>\n\n<style>\n  #label:hover{\n    cursor: pointer;\n    transform: scale(1.4);\n  }\n</style>"]},"metadata":{},"sourceType":"module"}