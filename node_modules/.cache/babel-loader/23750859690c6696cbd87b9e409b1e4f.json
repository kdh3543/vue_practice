{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n    const numberOfTodos = ref(0);\n    const currentPage = ref(1);\n    const limit = 5;\n    const numberOfPages = computed(() => {\n      return Math.ceil(numberOfTodos.value / limit);\n    });\n\n    const addTodo = async todo => {\n      try {\n        const res = await axios.post('http://localhost:3000/todos?_page=1&_limit=5', {\n          subject: todo.subject,\n          completed: todo.completed\n        });\n        todos.value.push(res.data);\n      } catch (err) {\n        error.value = err;\n      } // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n\n    };\n\n    const getTodos = async (page = currentPage) => {\n      currentPage.value = page;\n\n      try {\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`);\n        numberOfTodos.value = res.headers['x-total-count'];\n        todos.value = res.data;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    getTodos();\n\n    const toggleTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.patch('http://localhost:3000/todos/' + id, {\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const deleteTodo = async index => {\n      const id = todos.value[index].id;\n\n      try {\n        await axios.delete('http://localhost:3000/todos/' + id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      } catch (err) {\n        error.value = err;\n      }\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos,\n      numberOfPages,\n      currentPage\n    };\n  }\n\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMQ,KAAI,GAAIR,GAAG,CAAC,EAAD,CAAjB;AACA,UAAMS,aAAY,GAAIT,GAAG,CAAC,CAAD,CAAzB;AACA,UAAMU,WAAU,GAAIV,GAAG,CAAC,CAAD,CAAvB;AACA,UAAMW,KAAI,GAAI,CAAd;AACA,UAAMC,aAAY,GAAIX,QAAQ,CAAC,MAAM;AACnC,aAAOY,IAAI,CAACC,IAAL,CAAUL,aAAa,CAACM,KAAd,GAAoBJ,KAA9B,CAAP;AACD,KAF6B,CAA9B;;AAIA,UAAMK,OAAM,GAAI,MAAOC,IAAP,IAAgB;AAC9B,UAAG;AACD,cAAMC,GAAE,GAAI,MAAMd,KAAK,CAACe,IAAN,CAAW,8CAAX,EAA0D;AAC1EC,iBAAO,EAAEH,IAAI,CAACG,OAD4D;AAE1EC,mBAAS,EAAEJ,IAAI,CAACI;AAF0D,SAA1D,CAAlB;AAIAd,aAAK,CAACQ,KAAN,CAAYO,IAAZ,CAAiBJ,GAAG,CAACK,IAArB;AACD,OAND,CAMC,OAAMC,GAAN,EAAU;AACThB,aAAK,CAACO,KAAN,GAAcS,GAAd;AACF,OAT8B,CAU9B;AACA;AACA;AACA;AACA;;AACD,KAfD;;AAiBA,UAAMC,QAAO,GAAI,OAAMC,IAAG,GAAIhB,WAAb,KAA6B;AAC5CA,iBAAW,CAACK,KAAZ,GAAoBW,IAApB;;AACA,UAAG;AACD,cAAMR,GAAE,GAAI,MAAMd,KAAK,CAACuB,GAAN,CAAW,qCAAoCD,IAAK,WAAUf,KAAM,EAApE,CAAlB;AACAF,qBAAa,CAACM,KAAd,GAAsBG,GAAG,CAACU,OAAJ,CAAY,eAAZ,CAAtB;AACArB,aAAK,CAACQ,KAAN,GAAcG,GAAG,CAACK,IAAlB;AACD,OAJD,CAIC,OAAMC,GAAN,EAAU;AACThB,aAAK,CAACO,KAAN,GAAcS,GAAd;AACF;AACF,KATA;;AAUAC,YAAQ;;AAER,UAAMI,UAAS,GAAI,MAAOC,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIxB,KAAK,CAACQ,KAAN,CAAYe,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAM3B,KAAK,CAAC4B,KAAN,CAAY,iCAA+BD,EAA3C,EAA8C;AAClDV,mBAAS,EAAE,CAACd,KAAK,CAACQ,KAAN,CAAYe,KAAZ,EAAmBT;AADmB,SAA9C,CAAN;AAGAd,aAAK,CAACQ,KAAN,CAAYe,KAAZ,EAAmBT,SAAnB,GAA+B,CAACd,KAAK,CAACQ,KAAN,CAAYe,KAAZ,EAAmBT,SAAnD;AACD,OALD,CAKC,OAAMG,GAAN,EAAU;AACThB,aAAK,CAACO,KAAN,GAAcS,GAAd;AACF;AAED,KAXD;;AAaA,UAAMS,UAAS,GAAI,MAAOH,KAAP,IAAiB;AAClC,YAAMC,EAAC,GAAIxB,KAAK,CAACQ,KAAN,CAAYe,KAAZ,EAAmBC,EAA9B;;AACA,UAAG;AACD,cAAM3B,KAAK,CAAC8B,MAAN,CAAa,iCAA+BH,EAA5C,CAAN;AACAxB,aAAK,CAACQ,KAAN,CAAYoB,MAAZ,CAAmBL,KAAnB,EAA0B,CAA1B;AACAtB,aAAK,CAACO,KAAN,GAAc,EAAd;AACD,OAJD,CAIC,OAAMS,GAAN,EAAU;AACThB,aAAK,CAACO,KAAN,GAAcS,GAAd;AACF;AACD,KATD;;AAWA,UAAMY,UAAS,GAAIpC,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMqC,aAAY,GAAIpC,QAAQ,CAAC,MAAM;AACnC,UAAGmC,UAAU,CAACrB,KAAd,EAAoB;AAClB,eAAOR,KAAK,CAACQ,KAAN,CAAYuB,MAAZ,CAAmBrB,IAAG,IAAK;AAChC,iBAAOA,IAAI,CAACG,OAAL,CAAamB,QAAb,CAAsBH,UAAU,CAACrB,KAAjC,CAAP;AACD,SAFM,CAAP;AAGD,OAJD,MAIK;AACH,eAAOR,KAAK,CAACQ,KAAb;AACF;AACD,KAR6B,CAA9B;AASA,WAAO;AACLR,WADK;AAELS,aAFK;AAGLiB,gBAHK;AAILJ,gBAJK;AAKLO,gBALK;AAMLC,mBANK;AAOL7B,WAPK;AAQLiB,cARK;AASLb,mBATK;AAULF;AAVK,KAAP;AAYF;;AA1Fa,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","axios","components","setup","todos","error","numberOfTodos","currentPage","limit","numberOfPages","Math","ceil","value","addTodo","todo","res","post","subject","completed","push","data","err","getTodos","page","get","headers","toggleTodo","index","id","patch","deleteTodo","delete","splice","searchText","filteredTodos","filter","includes"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n\n    <h2>To-Do List</h2>\n    <input\n        class=\"form-control\"\n        type=\"text\" \n        v-model=\"searchText\"\n        placeholder=\"search\"\n    >\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    <div style=\"color:red\">{{error}}</div>\n    <div v-if=\"!filteredTodos.length\">\n      There is nothing\n    </div>\n    <TodoList :todos=\"filteredTodos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n    <hr>\n    <nav aria-label=\"Page navigation example\">\n      <ul class=\"pagination\">\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\" v-if=\"currentPage!=1\" @click=\"getTodos(currentPage+1)\">Previous</a></li>\n        <li class=\"page-item\" v-for=\"page in numberOfPages\" :key=\"page\" :class=\"currentPage===page ? 'active' : ''\"><a class=\"page-link\" href=\"#\" @click=\"getTodos(page)\">{{page}}</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\" v-if=\"currentPage!=numberOfPages\" @click=\"getTodos(currentPage+1)\">Next</a></li>\n      </ul>\n    </nav>\n  </div>\n</template>\n\n<script>\nimport { ref, computed} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n  setup() {\n    const todos = ref([]);\n    const error = ref('');\n    const numberOfTodos = ref(0);\n    const currentPage = ref(1);\n    const limit = 5;\n    const numberOfPages = computed(() => {\n      return Math.ceil(numberOfTodos.value/limit);\n    })\n\n    const addTodo = async (todo) => {\n      try{\n        const res = await axios.post('http://localhost:3000/todos?_page=1&_limit=5',{\n          subject: todo.subject,\n          completed: todo.completed,\n        });\n        todos.value.push(res.data);\n      }catch(err){\n        error.value = err;\n      } \n      // .then(res => {\n      //   todos.value.push(res.data);\n      // }).catch(err => {\n      //   \n      // });\n    };\n    \n    const getTodos = async(page = currentPage) => {\n      currentPage.value = page;\n      try{\n        const res = await axios.get(`http://localhost:3000/todos?_page=${page}&_limit=${limit}`)\n        numberOfTodos.value = res.headers['x-total-count'];\n        todos.value = res.data;\n      }catch(err){\n        error.value = err;\n      }      \n    }\n    getTodos();\n\n    const toggleTodo = async (index) => {\n      const id = todos.value[index].id;\n      try{\n        await axios.patch('http://localhost:3000/todos/'+id,{\n          completed: !todos.value[index].completed\n        });\n        todos.value[index].completed = !todos.value[index].completed;\n      }catch(err){\n        error.value = err;\n      }\n      \n    };\n\n    const deleteTodo = async (index) => {\n      const id = todos.value[index].id;\n      try{\n        await axios.delete('http://localhost:3000/todos/'+id);\n        todos.value.splice(index, 1);\n        error.value = '';\n      }catch(err){\n        error.value = err;\n      }      \n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if(searchText.value){\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }else{\n        return todos.value;\n      }\n    })\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos,\n      error,\n      getTodos,\n      numberOfPages,\n      currentPage\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}