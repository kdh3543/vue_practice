{"ast":null,"code":"import { ref, computed } from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = todo => {\n      axios.post('http://localhost:3000/todos', {\n        subject: todo.subject,\n        completed: todo.completed\n      }).then(res => {\n        console.log(res);\n        todos.value.push(todo);\n      });\n    };\n\n    const toggleTodo = index => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = index => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if (searchText.value) {\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      } else {\n        return todos.value;\n      }\n    });\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAqBA,SAASA,GAAT,EAAcC,QAAd,QAA6B,KAA7B;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,2BAArB;AACA,OAAOC,KAAP,MAAkB,OAAlB;AAEA,eAAe;AACbC,YAAU,EAAE;AACVH,kBADU;AAEVC;AAFU,GADC;;AAKbG,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIP,GAAG,CAAC,EAAD,CAAjB;;AAEA,UAAMQ,OAAM,GAAKC,IAAD,IAAU;AACxBL,WAAK,CAACM,IAAN,CAAW,6BAAX,EAAyC;AACvCC,eAAO,EAAEF,IAAI,CAACE,OADyB;AAEvCC,iBAAS,EAAEH,IAAI,CAACG;AAFuB,OAAzC,EAGGC,IAHH,CAGQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACAP,aAAK,CAACU,KAAN,CAAYC,IAAZ,CAAiBT,IAAjB;AACD,OAND;AAOD,KARD;;AAUA,UAAMU,UAAS,GAAKC,KAAD,IAAW;AAC5Bb,WAAK,CAACU,KAAN,CAAYG,KAAZ,EAAmBR,SAAnB,GAA+B,CAACL,KAAK,CAACU,KAAN,CAAYG,KAAZ,EAAmBR,SAAnD;AACD,KAFD;;AAIA,UAAMS,UAAS,GAAKD,KAAD,IAAW;AAC5Bb,WAAK,CAACU,KAAN,CAAYK,MAAZ,CAAmBF,KAAnB,EAA0B,CAA1B;AACD,KAFD;;AAIA,UAAMG,UAAS,GAAIvB,GAAG,CAAC,EAAD,CAAtB;AACA,UAAMwB,aAAY,GAAIvB,QAAQ,CAAC,MAAM;AACnC,UAAGsB,UAAU,CAACN,KAAd,EAAoB;AAClB,eAAOV,KAAK,CAACU,KAAN,CAAYQ,MAAZ,CAAmBhB,IAAG,IAAK;AAChC,iBAAOA,IAAI,CAACE,OAAL,CAAae,QAAb,CAAsBH,UAAU,CAACN,KAAjC,CAAP;AACD,SAFM,CAAP;AAGD,OAJD,MAIK;AACH,eAAOV,KAAK,CAACU,KAAb;AACF;AACD,KAR6B,CAA9B;AASA,WAAO;AACLV,WADK;AAELC,aAFK;AAGLa,gBAHK;AAILF,gBAJK;AAKLI,gBALK;AAMLC;AANK,KAAP;AAQF;;AA5Ca,CAAf","names":["ref","computed","TodoSimpleForm","TodoList","axios","components","setup","todos","addTodo","todo","post","subject","completed","then","res","console","log","value","push","toggleTodo","index","deleteTodo","splice","searchText","filteredTodos","filter","includes"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n\n    <h2>To-Do List</h2>\n    <input\n        class=\"form-control\"\n        type=\"text\" \n        v-model=\"searchText\"\n        placeholder=\"search\"\n    >\n    <hr>\n    <TodoSimpleForm @add-todo=\"addTodo\" />\n    \n    <div v-if=\"!filteredTodos.length\">\n      There is nothing\n    </div>\n    <TodoList :todos=\"filteredTodos\" @toggle-todo=\"toggleTodo\" @delete-todo=\"deleteTodo\"/>\n  </div>\n</template>\n\n<script>\nimport { ref, computed} from 'vue';\nimport TodoSimpleForm from './components/TodoSimpleForm.vue';\nimport TodoList from './components/TodoList.vue';\nimport axios from 'axios';\n\nexport default {\n  components: {\n    TodoSimpleForm,\n    TodoList\n  },\n  setup() {\n    const todos = ref([]);\n\n    const addTodo = (todo) => {\n      axios.post('http://localhost:3000/todos',{\n        subject: todo.subject,\n        completed: todo.completed,\n      }).then(res => {\n        console.log(res)\n        todos.value.push(todo);\n      });\n    };\n\n    const toggleTodo = (index) => {\n      todos.value[index].completed = !todos.value[index].completed;\n    };\n\n    const deleteTodo = (index) => {\n      todos.value.splice(index, 1);\n    };\n\n    const searchText = ref('');\n    const filteredTodos = computed(() => {\n      if(searchText.value){\n        return todos.value.filter(todo => {\n          return todo.subject.includes(searchText.value);\n        });\n      }else{\n        return todos.value;\n      }\n    })\n    return {\n      todos,\n      addTodo,\n      deleteTodo,\n      toggleTodo,\n      searchText,\n      filteredTodos\n    };\n  }\n}\n</script>\n\n<style>\n  .todo {\n    color: gray;\n    text-decoration: line-through;\n  }\n</style>"]},"metadata":{},"sourceType":"module"}