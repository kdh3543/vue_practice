{"ast":null,"code":"import { ref, computed } from 'vue';\nexport default {\n  emits: ['get-todo'],\n\n  setup(props, {\n    emit\n  }) {\n    const numberOfTodos = ref(0);\n    const currentPage = ref(1);\n    const limit = 5;\n    const numberOfPages = computed(() => {\n      return Math.ceil(numberOfTodos.value / limit);\n    });\n\n    const getTodos = (page = currentPage.value) => {\n      emit('get-todo', {\n        page: page,\n        limit: limit,\n        numberOfTodos: numberOfTodos.value\n      });\n    };\n\n    return {\n      numberOfPages,\n      numberOfTodos,\n      currentPage,\n      getTodos,\n      limit\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAWA,SAAQA,GAAR,EAAaC,QAAb,QAA4B,KAA5B;AACA,eAAe;AACXC,OAAK,EAAC,CAAC,UAAD,CADK;;AAEXC,OAAK,CAACC,KAAD,EAAO;AAACC;AAAD,GAAP,EAAc;AACf,UAAMC,aAAY,GAAIN,GAAG,CAAC,CAAD,CAAzB;AACA,UAAMO,WAAU,GAAIP,GAAG,CAAC,CAAD,CAAvB;AACA,UAAMQ,KAAI,GAAI,CAAd;AACA,UAAMC,aAAY,GAAIR,QAAQ,CAAC,MAAM;AACjC,aAAOS,IAAI,CAACC,IAAL,CAAUL,aAAa,CAACM,KAAd,GAAoBJ,KAA9B,CAAP;AACH,KAF6B,CAA9B;;AAGA,UAAMK,QAAO,GAAI,CAACC,IAAI,GAAEP,WAAW,CAACK,KAAnB,KAA6B;AAC1CP,UAAI,CAAC,UAAD,EAAY;AACZS,YAAI,EAAEA,IADM;AAEZN,aAAK,EAAEA,KAFK;AAGZF,qBAAa,EAAEA,aAAa,CAACM;AAHjB,OAAZ,CAAJ;AAKJ,KANA;;AAOA,WAAM;AACFH,mBADE;AAEFH,mBAFE;AAGFC,iBAHE;AAIFM,cAJE;AAKFL;AALE,KAAN;AAOJ;;AAvBW,CAAf","names":["ref","computed","emits","setup","props","emit","numberOfTodos","currentPage","limit","numberOfPages","Math","ceil","value","getTodos","page"],"sourceRoot":"","sources":["/Applications/kdh3543/vue_practice1/src/components/TodoPage.vue"],"sourcesContent":["<template>\n    <nav aria-label=\"Page navigation example\">\n      <ul class=\"pagination\">\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\" v-if=\"currentPage!=1\" @click=\"getTodos(currentPage-1)\">Previous</a></li>\n        <li class=\"page-item\" v-for=\"page in numberOfPages\" :key=\"page\" :class=\"currentPage===page ? 'active' : ''\"><a class=\"page-link\" href=\"#\" @click=\"getTodos(page)\">{{page}}</a></li>\n        <li class=\"page-item\"><a class=\"page-link\" href=\"#\" v-if=\"currentPage!=numberOfPages\" @click=\"getTodos(currentPage+1)\">Next</a></li>\n      </ul>\n    </nav>\n</template>\n\n<script>\nimport {ref, computed} from 'vue';\nexport default {\n    emits:['get-todo'],\n    setup(props,{emit}){\n        const numberOfTodos = ref(0);\n        const currentPage = ref(1);\n        const limit = 5;\n        const numberOfPages = computed(() => {\n            return Math.ceil(numberOfTodos.value/limit);\n        });\n        const getTodos = (page= currentPage.value) => {\n            emit('get-todo',{\n                page: page,\n                limit: limit,\n                numberOfTodos: numberOfTodos.value\n            })\n        }     \n        return{\n            numberOfPages,\n            numberOfTodos,\n            currentPage,\n            getTodos,\n            limit,\n        }\n    }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}